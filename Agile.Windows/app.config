<?xml version="1.0"?>
<configuration>
  <configSections>
    <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
      <section name="Agile.Windows.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false"/>
    </sectionGroup>
  </configSections>
  <connectionStrings>
    <add name="Agile.Windows.Properties.Settings.LocalDatabaseConnectionString" connectionString="Data Source=(localdb)\v11.0;Initial Catalog=Agile_Local;Integrated Security=true"/>
  </connectionStrings>
  <system.serviceModel>
    <bindings>

      <wsHttpBinding>
        <binding name="friendBinding" closeTimeout="01:00:00" openTimeout="01:00:00" receiveTimeout="01:00:00" sendTimeout="01:00:00" maxReceivedMessageSize="838860800">
          <readerQuotas maxArrayLength="2147483647" maxStringContentLength="2147483647"/>
        </binding>
      </wsHttpBinding>

      <basicHttpBinding>
        <binding name="transferBinding" maxReceivedMessageSize="10067108864" messageEncoding="Mtom" transferMode="Streamed">
        </binding>
      </basicHttpBinding>

      <netNamedPipeBinding>
        <binding name="AgileLocalBinding" maxReceivedMessageSize="2147483647" closeTimeout="00:05:00" openTimeout="00:05:00" receiveTimeout="00:20:00" sendTimeout="00:05:00">
          <readerQuotas maxArrayLength="2147483647" maxStringContentLength="2147483647"/>
        </binding>
      </netNamedPipeBinding>

    </bindings>
    <client>
      <endpoint address="http://localhost:7654/ServerAgile.svc" binding="wsHttpBinding" bindingConfiguration="friendBinding" contract="Agile.Services.IServerAgile" name="server"/>
      <endpoint address="http://localhost:7654/ServerAgileTransfer.svc" binding="basicHttpBinding" bindingConfiguration="transferBinding" contract="Agile.Services.IServerAgileTransfer" name="serverTransfer"/>

      <endpoint address="net.pipe://localhost/ServerAgileLocal" binding="netNamedPipeBinding" bindingConfiguration="AgileLocalBinding" contract="Agile.Services.IServerAgile" name="local"/>
    </client>


    <services>
      <service name="Agile.Local.ServerAgileLocal">
        <host>
          <baseAddresses>
            <add baseAddress="net.pipe://localhost/ServerAgileLocal"/>
          </baseAddresses>
        </host>
      </service>
    </services>
    <behaviors>
      <serviceBehaviors>
      </serviceBehaviors>
    </behaviors>


  </system.serviceModel>
  <!--Uncomment to trace WCF info -->
  <!--<system.diagnostics>
    <sources>
      <source name="System.ServiceModel" switchValue="Error" propagateActivity="false">
        <listeners>
          <add type="System.Diagnostics.DefaultTraceListener" name="Default">
            <filter type=""/>
          </add>
          <add initializeData="Traces.svclog" type="System.Diagnostics.XmlWriterTraceListener" name="traceListener" traceOutputOptions="None">
            <filter type=""/>
          </add>
        </listeners>
      </source>
    </sources>
    <trace autoflush="true"/>
  </system.diagnostics>-->
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.1"/>
  </startup>
  <userSettings>
    <Agile.Windows.Properties.Settings>
      <setting name="UserName" serializeAs="String">
        <value/>
      </setting>
    </Agile.Windows.Properties.Settings>
  </userSettings>
</configuration>
