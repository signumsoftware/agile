@using Agile.Entities

@using (var cc = Html.TypeContext<CompanyEntity>()) 
{
	@Html.ValueLine(cc, c => c.CompanyName)

	@Html.ValueLine(cc, c => c.ContactName)
	@Html.ValueLine(cc, c => c.ContactTitle)
  
	@Html.ValueLine(cc, c => c.Phone)
	@Html.ValueLine(cc, c => c.Fax)

    
    @Html.EmbeddedControl(cc, c => c.Address)  
}
